    <tr>
        <th scope="row">GPU</th>
        <td><%= gpu.generalGPU %></td>
    </tr>
    <tr>
        <th scope="row">Type</th>
        <td><%= gpu.generalType %></td>
    </tr>
    <tr>
        <th scope="row">Max Resolution</th>
        <td><%= gpu.generalMaxRes %></td>
