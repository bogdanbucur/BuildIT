<h4 class="text-center"><%= gpu.description %></h4>
<table class="table table-striped">
    <thead>
    <tr>
        <th class="text-primary">General</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <th scope="row">Slot</th>
        <td><%= gpu.generalSlot %></td>
