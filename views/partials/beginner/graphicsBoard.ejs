    <thead>
    <tr>
        <th class="text-primary">Cooling</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <th scope="row">Type</th>
        <td><%= gpu.coolingType %></td>
    </tr>
    <tr>
        <th scope="row">Cooler</th>
        <td><%= gpu.coolingCooler %></td>
    </tr>
