<div class="col-sm-12">
    <input id="filterCases" type="text" class="form-control pull-right" placeholder="Type here..." style="width: 25%">
    <table class="table table-hover table-responsive">
        <thead>
        <tr>
            <th style="width: 10%"></th>
            <th style="width: 15%">Name</th>
            <th style="width: 10%">Type</th>
            <th style="width: 10%">Power Supply</th>
            <th style="width: 10%">Motherboard</th>
            <th style="width: 10%">Included Fans</th>
            <th style="width: 10%">Size</th>
            <th style="width: 10%">Rating</th>
            <th style="width: 10%">Price</th>
        </tr>
        </thead>
        <tbody class="searchable8" id="casesTable">
        <%casesArray.forEach(function(item) {%>
        <tr>
            <th scope="row" style="width: 10%">
                <img src="<%= item.image0 %>" alt="" height="64" width="64">
            </th>
            <td style="width: 15%">
                <%= item.name %>
            </td>
            <td style="width: 10%">
                <%= item.type %>
            </td>
            <td style="width: 10%">
                <%= item.powerSupply %>
            </td>
            <td style="width: 10%">
                <%= item.motherboardCompatibility %>
            </td>
            <td style="width: 10%">
                <%= item.includedFans %>
            </td>
            <td style="width: 10%">
                <%= item.size %>
            </td>
            <td style="width: 10%">
                <%= item.rating %> <li class="fa fa-star" style="color: gold"></li>
            </td>
            <td style="width: 10%">
                <%= item.price %>
            </td>
            <td style="width: 5%">

                <% include ../componentDetails/computerCaseDetails.ejs %>

            </td>
            <td style="width: 5%">
                <div class="choice" data-toggle="wizard-checkbox">
                    <input class="messageCheckbox" type="checkbox" title="Add me" value="<%= item._id %>^<%= item.colType %>">
                    <div class="icon">
                        <i class="fa fa-check-square-o"></i>
                    </div>
                </div>
            </td>
        </tr>
        <%})%>
        </tbody>
    </table>
    <table class="footer">
        <div class="text-center">
            <ul class="pagination pagination-lg pager" id="casesPager"></ul>
        </div>
    </table>
</div>