<div class="col-sm-12">
    <input id="filterBoards" type="text" class="form-control pull-right" placeholder="Type here..." style="width: 25%">
    <table class="table table-hover table-responsive">
        <thead>
        <tr>
            <th style="width: 10%"></th>
            <th style="width: 60%">Description</th>
            <th style="width: 10%">Rating</th>
            <th style="width: 10%">Price</th>
        </tr>
        </thead>
        <tbody class="searchable1" id="boardsTable">
        <% boardArray.forEach(function(item) { %>
        <tr>
            <th scope="row" style="width: 10%">
                <img src="<%= item.image0 %>" alt="" height="64" width="64">
            </th>
            <td style="width: 60%">
                <%= item.description %>
            </td>
            <td style="width: 10%">
                <%= item.rating %>
                <li class="fa fa-star" style="color: gold"></li>
            </td>
            <td style="width: 10%">
                <%= item.price %>
            </td>
            <td style="width: 5%">
                <% include ../componentDetails/motherboardDetails.ejs %>
            </td>
            <td style="width: 5%">
                <div class="choice" data-toggle="wizard-checkbox">
                    <input class="messageCheckbox" id="check<%= item._id %>" type="checkbox" title="Add me" value="<%= item._id %>^<%= item.colType %>">
                    <label for="check<%= item._id %>">
                        <i class="fa fa-check-square-o"></i>
                    </label>
                </div>
            </td>
        </tr>
        <% }) %>
        </tbody>
    </table>
    <div class="text-center">
        <ul class="pagination pagination-sm pager" id="boardsPager"></ul>
    </div>
</div>