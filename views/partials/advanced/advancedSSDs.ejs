<div class="col-sm-12">
    <input id="filterSsd" type="text" class="form-control pull-right" placeholder="Type here..." style="width: 25%">
    <table class="table table-hover table-responsive">
        <thead>
        <tr>
            <th style="width: 10%"></th>
            <th style="width: 15%">Name</th>
            <th style="width: 10%">Form Factor</th>
            <th style="width: 10%">Capacity</th>
            <th style="width: 10%">Read</th>
            <th style="width: 10%">Write</th>
            <th style="width: 10%">Type</th>
            <th style="width: 10%">Rating</th>
            <th style="width: 10%">Price</th>
        </tr>
        </thead>
        <tbody class="searchable5" id="ssdTable">
        <% ssdArray.forEach(function(item) { %>
        <tr>
            <th scope="row" style="width: 10%">
                <img src="<%= item.image0 %>" alt="" height="64" width="64">
            </th>
            <td style="width: 15%">
                <%= item.name %>
            </td>
            <td style="width: 10%">
                <%= item.formFactor %>
            </td>
            <td style="width: 10%">
                <%= item.capacity %>
            </td>
            <td style="width: 10%">
                <%= item.readTransferRate %>
            </td>
            <td style="width: 10%">
                <%= item.writeTransferRate %>
            </td>
            <td style="width: 10%">
                <%= item.type %>
            </td>
            <td style="width: 10%">
                <%= item.rating %>
                <li class="fa fa-star" style="color: gold"></li>
            </td>
            <td style="width: 10%">
                <%= item.price %>
            </td>
            <td style="width: 5%">
                <% include ../componentDetails/ssdDetails.ejs %>
            </td>
            <td style="width: 5%">
                <div class="choice" data-toggle="wizard-checkbox">
                    <input class="messageCheckbox" type="checkbox" title="Add me" value="<%= item._id %>^<%= item.colType %>">
                    <div class="icon">
                        <i class="fa fa-check-square-o"></i>
                    </div>
                </div>
            </td>
        </tr>
        <% }) %>
        </tbody>
    </table>
    <div class="text-center">
        <ul class="pagination pagination-sm1 pager" id="ssdPager"></ul>
    </div>
</div>